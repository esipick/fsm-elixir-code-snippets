<!doctype html>
<html lang="en">
  <head>
    <title>Randon Aviation</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900" rel="stylesheet">
    <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" /> -->
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    <link rel="stylesheet" href="<%= static_path(@conn, "/css/admin/admin.css") %>">
    <script src="<%= static_path(@conn, "/js/admin/admin.js") %>"></script>
  </head>
  <body>
    <div class="wrapper">

    <%= render FlightWeb.Admin.PartialView, "_sidebar.html", assigns %>
    <div class="main-panel">
      <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-transparent  navbar-absolute bg-primary fixed-top">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            <div class="navbar-toggle">
              <button type="button" class="navbar-toggler">
                <span class="navbar-toggler-bar bar1"></span>
                <span class="navbar-toggler-bar bar2"></span>
                <span class="navbar-toggler-bar bar3"></span>
              </button>
            </div>
            <a class="navbar-brand" href="#pablo">Flight School Manager</a>
          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navigation">

          </div>
        </div>
      </nav>
      <!-- End Navbar -->
      <div class="panel-header panel-header-sm">
        <!-- <div class="header text-center">
          <h2 class="title">Student Management</h2>
          <p class="category">A list of all Randon Aviation students.</p>
        </div> -->
      </div>

      <div class="content">

        <%= render @view_module, @view_template, assigns %>
      </div>

        <footer class="footer" >
          <div class="container-fluid">
            <nav>
              <ul>
                <li>
                  <a href="#e">
                    Privacy Policy
                  </a>
                </li>
                <li>
                  <a href="#e">
                     Terms of Use
                  </a>
                </li>
                <li>
                  <a href="#e">
                     Contact Us
                  </a>
                </li>
              </ul>
            </nav>
            <div class="copyright">
              &copy; <script>document.write(new Date().getFullYear())</script>, Flight School Manager
            </div>
          </div>
        </footer>

      </div>
    </div>
  </body>

</html>
